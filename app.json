{
  "expo": {
    "name": "BaixaTudo",
    "slug": "BaixaTudo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "baixatudo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1D1D1D" 
      },
      "edgeToEdgeEnabled": true,
      "package": "com.luizf.baixatudo",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1D1D1D"
        }
      ],
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": true,
          "savePhotosOnAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8b56b065-87d6-4b13-9cbb-b415561aa804"
      }
    }
  }
}
